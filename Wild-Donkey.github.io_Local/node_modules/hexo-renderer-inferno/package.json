{
  "name": "hexo-renderer-inferno",
  "version": "0.1.3",
  "description": "Inferno.js renderer for Hexo",
  "keywords": [
    "hexo",
    "inferno",
    "renderer",
    "jsx"
  ],
  "main": "index.js",
  "author": "ppoffice <zrp1994@gmail.com>",
  "maintainers": [
    "ppoffice <zrp1994@gmail.com>"
  ],
  "license": "MIT",
  "repository": "https://github.com/ppoffice/hexo-renderer-inferno.git",
  "bugs": {
    "url": "https://github.com/vanwagonet/hexo-renderer-react/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "mocha test/index.js",
    "test-cov": "nyc npm run test",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION && git push --tags"
  },
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "index.js",
    "lib/"
  ],
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "babel-plugin-inferno": "^6.1.0",
    "inferno": "^7.3.3",
    "inferno-create-element": "^7.3.3",
    "inferno-server": "^7.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-hexo": "^4.1.0",
    "eslint-plugin-react": "^7.17.0",
    "hexo-fs": "^2.0.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8"
  }
